{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve -H 0.0.0.0",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "babel-plugin-styled-components": "^2.0.7",
    "gatsby": "^5.0.1",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-google-gtag": "^5.0.0",
    "gatsby-plugin-manifest": "^5.0.0",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^6.0.0",
    "gatsby-plugin-styled-components": "^6.0.0",
    "gh-pages": "^4.0.0",
    "react": "^18.2.0",
    "react-burger-menu": "^3.0.8",
    "react-burgers": "^1.3.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-particles": "^2.5.3",
    "react-scroll": "^1.8.8",
    "rellax": "^1.12.1",
    "styled-components": "^5.3.6",
    "tsparticles": "^2.5.3"
  },
  "devDependencies": {
    "prettier": "^2.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfekPeres/Portfolio/"
  },
  "bugs": {
    "url": "https://github.com/OfekPeres/Portfolio/issues"
  }
}
